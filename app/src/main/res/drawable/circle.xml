<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- 设置填充颜色 为白色 -->
    <solid android:color="@android:color/transparent" />
    <!-- 设置几何图形四个角的弧度 -->
    <corners android:radius="18dp"/>
    <!-- 设置几何形状绘制边框 红色边框 -->
    <stroke android:color="#FFC94136"
        android:width="2dp"/>
</shape>